<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Start Code Generation"
            visible="true"
            id="de.cognicrypt.codegenerator.actionSet">
         <menu
               label="CogniCryptMenu"
               id="sampleMenu">
            <separator
                  name="CognicryptGroup">
            </separator>
         </menu>
         <action
               class="de.cognicrypt.codegenerator.actions.WizardAction"
               icon="icons/cognicrypt.png"
               id="de.cognicrypt.codegenerator.actions.WizardAction"
               label="&amp;Start Code Generation"
               menubarPath="CogniCryptMenu/CogniCryptGroup"
               toolbarPath="CogniCryptGroup"
               tooltip="Generate Code For Cryptographic Task">
         </action>
      </actionSet>
   </extension>
	<extension
         id="QuickFixTest.ProblemMarkerBuilder"
         name="ProblemMarkerBuilder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="quickfixtest.builder.ProblemMarkerBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="QuickFixTest.ProblemMarkerNature"
         name="ProblemMarkerNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="quickfixtest.builder.ProblemMarkerNature">
         </run>
      </runtime>
      <builder
            id="QuickFixTest.ProblemMarkerBuilder">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="ProblemMarkerNature commands"
            id="QuickFixTest.ProblemMarkerNature.category">
      </category>
      <command
            name="Add/RemoveproblemMarkerNature"
            defaultHandler="quickfixtest.builder.AddRemoveproblemMarkerNatureHandler"
            categoryId="QuickFixTest.ProblemMarkerNature.category"
            id="QuickFixTest.AddRemoveproblemMarkerNature">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.projectConfigure?after=additions">
         <command
               commandId="QuickFixTest.AddRemoveproblemMarkerNature"
               label="Disable QF builder"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                        <test
                              value="QuickFixTest.ProblemMarkerNature"
                              property="org.eclipse.core.resources.projectNature">
                        </test>
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="QuickFixTest.AddRemoveproblemMarkerNature"
               label="Enable QF builder"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                        <not>
                           <test
                                 value="QuickFixTest.ProblemMarkerNature"
                                 property="org.eclipse.core.resources.projectNature">
                           </test>
                        </not>
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         id="QuickFixTest.OCCEProblem"
         name="OCCE Problem"
         point="org.eclipse.core.resources.markers">
         <super 
         	type="org.eclipse.core.resources.textmarker">
         </super>
         <super
          	type="org.eclipse.jdt.core.problem">
    	</super>
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
	<extension
      point="org.eclipse.jdt.ui.quickFixProcessors">
   		<quickFixProcessor
         	class="quickfixtest.quickfix.QuickFixProcessor"
         	id="org.eclipse.jdt.ui.text.correction.QuickFixProcessor"
         	name="QuickFixProcessor">
      	<handledMarkerTypes>
         	<markerType
               id="QuickFixTest.OCCEProblem">
         	</markerType>
      	</handledMarkerTypes>
   		</quickFixProcessor>
	</extension>

<extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="code-clafer_configurator.objectContribution"
            objectClass="org.eclipse.core.resources.IResource">
         <menu
               icon="icons/cognicrypt.png"
               id="code-clafer_configurator.menu1"
               label="Launch CogniCrypt">
            <separator
                  name="code-clafer_configurator.separator1">
            </separator>
         </menu>
         <action
               class="de.cognicrypt.codegenerator.actions.WizardActionFromContextMenu"
               icon="icons/cognicrypt.png"
               id="code-clafer_configurator_fromContextMenu"
               label="Launch CogniCrypt">
         </action>
      </objectContribution>
   </extension>
   
   <extension
       point="org.eclipse.help.contexts">
        <contexts file="HelpContexts_1.xml"/>
        <contexts file="HelpContexts_2.xml"/> 
   </extension>

<extension
      point="org.eclipse.help.toc">
      <toc file="toc.xml" primary="true" />
      <toc file="toc_Tasks.xml" />
      <toc file="toc_TaskConfiguration.xml" />
   </extension>
 
     
</plugin>
