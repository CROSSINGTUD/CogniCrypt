Content-Type: text/ecl
Entry-Name: .content

get-button "Generate Code For Cryptographic Task" | click
with [get-window "Cryptography Task Configurator"] {
    get-button "Primitive Integration" | click
    with [get-window -class WizardDialog] {
        get-button "Next >" | click
        get-button Symmetric | click
        get-button Block | click
        get-button "Next >" | click
        with [get-editbox -after [get-label "Please enter the name of your algorithm"]] {
            set-text SICS
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-editbox -after [get-label "What is the name of the author/company?"]] {
            set-text UPB
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-editbox -after [get-label "Please give a short description of your algorithm"] 
            | set-text "This is the demo for UI testing using RCPTT Plugin tool."
        get-button "Next >" | click
        get-combo -after [get-label "How many keys include your algorithm ?"] | select 3
        get-editbox -after [get-label "What is the block size of the cipher ?"] | click-text
        get-combo -after [get-label "How many keys include your algorithm ?"] | select 2
        get-editbox -after [get-label "What is the block size of the cipher ?"] | set-text 1
        get-button "Next >" | click
        get-group "" | get-button "fixed size" | click
        get-group "" -after [get-label "Please enter the key size of your algorithm"] -index 1 
            | get-button "Ranged size" | click
        get-group "" | get-editbox | set-text 128
        with [get-group "" -after [get-label "Please enter the key size of your algorithm"] -index 1] {
            with [get-editbox] {
                set-text 256
                key-type "TRAVERSE_TAB_NEXT"
            }
            get-editbox -index 1 | set-text 512
        }
        get-button "Next >" | click
        get-button CBC | check
        get-button CTR | check
        get-button "Next >" | click
        get-combo -after [get-label "Please select the padding used for your algorithm"] | select PKCS7
        get-button "Next >" | click
        get-button Finish | click
    }
    get-button Cancel | click
}
