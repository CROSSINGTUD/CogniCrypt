Content-Type: text/ecl
Entry-Name: .content

get-button "Generate Code For Cryptographic Task" | click
with [get-window "Cryptography Task Configurator"] {
    get-control Any -index 5 | key-type "" "\\u0000"
    get-button "Primitive Integration" | click
    with [get-window -class WizardDialog] {
        get-combo -after [get-label "Please choose the type of algorithm"] | select "Symmetric Block Cipher"
        get-button "Next >" | click
        get-editbox -after [get-label "Please enter the name of your algorithm"] | set-text Sample
        with [get-editbox -after [get-label "What is the name of the author/company?"]] {
            set-text UPB
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-editbox -after [get-label "Please give a short description of your algorithm"] 
            | set-text "This is a Sample Testing"
        get-button "Next >" | click
        get-editbox -after [get-label "What is the block size of the cipher ?(bits)"] | set-text 458
        get-button "Next >" | click
        with [get-group ""] {
            get-button "fixed size" | click
            get-editbox | set-text 123
        }
        get-button "Next >" | click
        get-button CBC | check
        get-button CTR | check
        get-button CFB | check
        get-button "Next >" | click
        get-button NoPadding | check
        get-button PKCS7 | check
        get-button "Next >" | click
    }
}
set-dialog-result File
with [get-window "Cryptography Task Configurator"] {
    with [get-window -class WizardDialog] {
        get-button Browse | click
        get-button Cancel | click
        get-window Confirmation | get-button OK | click
    }
    get-button Cancel | click
}
