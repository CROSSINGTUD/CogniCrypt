Content-Type: text/ecl
Entry-Name: .content

get-button "Integrate a Cryptographic Task into CogniCrypt" | click
with [get-window -class WizardDialog] {
    with [get-group "Choose the mode of this Wizard"] {
        get-editbox -after [get-label "Name of the Task"] | set-text DependencyDialog
        get-editbox -after [get-label "Description of the Task :"] | click-text
    }
    with [get-button "Next >"] {
        click
        click
    }
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        with [get-editbox -after [get-label Question]] {
            set-text Q0
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-combo -after [get-label "Answer type"]] {
            select "Drop down"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-button "Add Answer"] {
            click
            click
        }
        get-group "" | get-editbox | set-text ans1
        get-group "" -after [get-label "Answer type"] -index 1 | get-editbox | set-text ans2
        get-button OK | click
    }
    get-button "Click toLink Variabilityconstruct and  Link code" | click
    with [get-window -class AddDependenciesDialog] {
        get-group "" -after [get-label Q0] -index 1 | get-button Add | click
        get-group "" -after [get-label Q0] -index 1 | get-button Add | click
        get-group "" -after [get-label Q0] -index 1 | get-button Add | click
        get-group "" -after [get-label Q0] -index 5 | get-button Add | click
        get-group "" -after [get-label Q0] -index 5 | get-button Add | click
    }
}
with [get-window -class WizardDialog] {
    with [get-window -class AddDependenciesDialog] {
        get-group "" -after [get-label Q0] -index 1 | get-group "" -index 1 | get-button Remove | click
        get-group "" -after [get-label Q0] -index 1 | get-group "" -index 1 | get-button Remove | click
        get-button OK | click
    }
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        with [get-editbox -after [get-label Question]] {
            set-text Q0
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-combo -after [get-label "Answer type"]] {
            select "Drop down"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-button "Add Answer"] {
            click
            click
        }
        get-group "" | get-editbox | set-text ans1
        get-group "" -after [get-label "Answer type"] -index 1 | get-editbox | click-text
        get-editbox -after [get-label Question] | set-text Q1
        get-combo -after [get-label "Answer type"] | select "Radio button"
        get-button "Add Answer" | click
        get-group "" | get-editbox | set-text ans1
        get-button "Add Answer" | click
        get-group "" -after [get-label "Answer type"] -index 1 | get-editbox | set-text ans2
        get-button OK | click
    }
    get-button "Click toLink Variabilityconstruct and  Link code" -index 1 | click
    with [get-window -class AddDependenciesDialog] {
        get-group "" -after [get-label Q1] -index 1 | get-button Add | click
        get-group "" -after [get-label Q1] -index 1 | get-button Add | click
        get-group "" -after [get-label Q1] -index 1 | get-button Add | click
        get-group "" -after [get-label Q1] -index 5 | get-button Add | click
        get-group "" -after [get-label Q1] -index 5 | get-button Add | click
        get-group "" -after [get-label Q1] -index 5 | get-button Add | click
        get-group "" -after [get-label Q1] -index 1 | get-group "" -index 1 | get-button Remove | click
        get-group "" -after [get-label Q1] -index 1 | get-group "" -index 1 | get-button Remove | click
        get-group "" -after [get-label Q1] -index 3 | get-group "" | get-button Remove | click
        get-group "" -after [get-label Q1] -index 1 | get-group "" | get-button Remove | click
        get-button OK | click
    }
    get-button "Click toLink Variabilityconstruct and  Link code" -index 1 | click
    with [get-window -class AddDependenciesDialog] {
        get-tab-folder | get-tab-item "Link to variables to use in code" | click
        get-group "" -after [get-label Q1] -index 1 | get-editbox -index 2 | set-text fedfe
        get-group "" -after [get-label Q1] -index 2 | get-editbox -index 2 | set-text sevf
        get-button OK | click
    }
}
