Content-Type: text/ecl
Entry-Name: .content

get-button "Integrate a Cryptographic Task into CogniCrypt" | click
with [get-window -class WizardDialog] {
    with [get-group "Choose the mode of this Wizard"] {
        get-editbox -after [get-label "Name of the Task"] | set-text Assertest
        with [get-editbox -after [get-label "Description of the Task :"]] {
            set-text "Assertion test"
            key-type Enter
            set-text "Assertion test\n"
            key-type Enter
        }
    }
    get-button "Next >" | click
}
get-window -class WizardDialog | get-button "Add Feature" | get-property "isEnabled()" | equals false | verify-true
with [get-window -class WizardDialog] {
    get-button "Add Feature" | click
    with [get-window -class ClaferFeatureDialog] {
        with [get-editbox -after [get-label "Type in the name"]] {
            set-text sdcvds
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-combo -after [get-label "Inherits from"] | select ""
        get-button OK | click
    }
    get-button "Add Pattern" | click
    with [get-window -class ClaferFeaturePatternDialog] {
        with [get-editbox -after [get-label Name]] {
            set-text string
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-button "Add option"] {
            key-type "TRAVERSE_TAB_NEXT"
            click
        }
        with [get-editbox -after [get-label Name] -index 1] {
            set-text sd
            set-text sdcs
        }
        get-button OK | click
    }
    get-button "Import Features" | click
    get-window -class ClaferImportDialog | get-button Cancel | click
}
