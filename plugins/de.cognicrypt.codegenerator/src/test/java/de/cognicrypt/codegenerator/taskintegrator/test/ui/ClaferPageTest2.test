Entry-Name: .content

get-button "Integrate a Cryptographic Task into CogniCrypt" | click
with [get-window -class WizardDialog] {
    with [get-group "Choose the mode of this Wizard"] {
        with [get-editbox -after [get-label "Name of the Task"]] {
            set-text "Clafer page test"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-editbox -after [get-label "Description of the Task :"]] {
            set-text "Clafer page widget test"
            key-type Left -times 5
            set-text "Clafer page widgets test"
            key-type Right -times 2
            key-type Enter
            set-text "Clafer page widgets t\n"
                + "est"
            key-type Enter
            set-text "Clafer page widgets test"
        }
    }
    get-button "Next >" | click
    get-button "Add Feature" | click
    get-window -class ClaferFeatureDialog | get-button Cancel | click
    get-button "Add Pattern" | click
    with [get-window -class ClaferFeaturePatternDialog] {
        get-editbox -after [get-label Name] | set-text items
        get-button "Add option" | click
        get-editbox -after [get-label Name] -index 1 | set-text combo
        get-button "Add option" | click
        get-editbox -after [get-label Name] -index 2 | set-text text
        get-button OK | click
    }
    get-button "Add Pattern" | click
    with [get-window -class ClaferFeaturePatternDialog] {
        get-combo -after [get-label Pattern] | select "Ordered Enumeration"
        get-editbox -after [get-label Name] | set-text answers
        get-button "Add option" | click
        get-editbox -after [get-label Name] -index 1 | set-text ans1
        get-button "Add option" | click
        get-editbox -after [get-label Name] -index 2 | set-text asn2
        get-button OK | click
    }
    get-button "Add Feature" | click
    with [get-window -class ClaferFeatureDialog] {
        with [get-editbox -after [get-label "Type in the name"]] {
            click-text
            key-type "M1+v"
            set-text Feature1
        }
        get-combo -after [get-label "Inherits from"] | select answers
        get-button "Add property" | click
        get-editbox -after [get-label Name] | set-text asca
        get-combo -after [get-label "is of type"] | select ans1
        get-button OK | click
    }
    get-button Modify -index 6 | click
    get-window -class ClaferFeatureDialog | get-button OK | click
    get-button Modify -index 6 | click
    with [get-window -class ClaferFeatureDialog] {
        get-button Remove | click
        get-button OK | click
    }
}
set-dialog-result File
get-window -class WizardDialog | get-button "Export Features" | click
set-dialog-result MessageBox 64
with [get-window -class WizardDialog] {
    get-button Delete -index 3 | click
    get-button Modify | click
    get-window -class ClaferFeatureDialog | get-editbox -after [get-label "Type in the name"] | set-text itemsssss
}
set-dialog-result MessageBox 64
get-window -class WizardDialog | get-window -class ClaferFeatureDialog | get-button OK | click
get-window -class WizardDialog | get-button "Import Features" | click
set-dialog-result File
with [get-window -class WizardDialog | get-window -class ClaferImportDialog] {
    get-button Browse | click
    get-button OK | click
}
set-dialog-result File
get-window -class WizardDialog | get-button "Export Features" | click
