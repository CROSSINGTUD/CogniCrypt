------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-button "Integrate a Cryptographic Task into CogniCrypt" | click
with [get-window -class WizardDialog] {
    with [get-group "Choose the mode of this Wizard"] {
        get-editbox -after [get-label "Name of the Task"] | set-text "High level question Page"
        get-editbox -after [get-label "Description of the Task :"] | set-text ""
        with [get-editbox -after [get-label "Name of the Task"]] {
            click-text 25
            key-type "M1+a"
            key-type "M1+c"
        }
        with [get-editbox -after [get-label "Description of the Task :"]] {
            click-text
            key-type "M1+v"
            set-text "High level question Page test "
            key-type Left -times 5
            set-text "High level question Page widgest test "
            key-type Enter
            set-text "High level question Page widgest \n"
                + "test "
            key-type Enter
            set-text "High level question Page widgest test "
            set-text "High level question Page widget test "
            key-type Right
            set-text "High level question Page widgets test "
        }
    }
    with [get-button "Next >"] {
        click
        click
    }
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        with [get-editbox -after [get-label Question]] {
            set-text Q0
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-combo -after [get-label "Answer type"]] {
            select "Drop down"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-button "Add Answer"] {
            click
            click
        }
        get-group "" | get-editbox | set-text ascd
        get-group "" -after [get-label "Answer type"] -index 1 | get-editbox | set-text sdvfv
        get-group "" | get-button "Default Answer" | click
    }
}
get-window -class WizardDialog | get-window -class QuestionDialog | get-button "Add Answer" 
    | get-property "isVisible()" | equals true | verify-true
with [get-window -class WizardDialog] {
    get-window -class QuestionDialog | get-button OK | click
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        with [get-editbox -after [get-label Question]] {
            set-text Q1
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-combo -after [get-label "Answer type"] | select "Text box"
        get-button OK | click
    }
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        with [get-editbox -after [get-label Question]] {
            set-text Q2
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-combo -after [get-label "Answer type"]] {
            select "Radio button"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-button "Add Answer"] {
            click
            click
        }
        with [get-group "" | get-editbox] {
            set-text ans1
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-group "" -after [get-label "Answer type"] -index 1 | get-editbox | set-text Ans2
        get-group "" | get-button "Default Answer" | click
        get-button OK | click
    }
    get-group "" | get-button Modify | click
    with [get-window -class QuestionDialog] {
        get-button "Add Answer" | click
        get-group "" -after [get-label "Answer type"] -index 2 | get-editbox | set-text answ2
    }
}
set-dialog-result MessageBox 64
with [get-window -class WizardDialog] {
    with [get-window -class QuestionDialog] {
        get-group "" -after [get-label "Answer type"] -index 2 | get-button Remove | click
        get-button OK | click
    }
    get-group "" -index 1 | get-button Down | click
}
set-dialog-result MessageBox 64
get-window -class WizardDialog | get-group "" | get-button Delete | click
with [get-window -class WizardDialog] {
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        with [get-editbox -after [get-label Question]] {
            set-text Q1
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-combo -after [get-label "Answer type"]] {
            select "Drop down"
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-button "Add Answer"] {
            click
            click
        }
        get-combo -after [get-label "Answer type"] | select "Radio button"
        get-button "Add Answer" | click
        get-group "" | get-editbox | set-text ans1
        get-button "Add Answer" | click
        get-group "" -after [get-label "Answer type"] -index 1 | get-editbox | set-text ans2
        get-button OK | click
    }
    get-button "Add Question" | click
    with [get-window -class QuestionDialog] {
        get-editbox -after [get-label Question] | set-text Q4
        get-combo -after [get-label "Answer type"] | select "Text box"
        get-button OK | click
    }
    get-button "Next >" | click
    get-button "Link Answer" | click
}
get-window -class WizardDialog | get-window -class LinkAnswerDialog | get-group "" -after [get-label Q0] -index 1 
    | get-combo | get-property "getItemCount()" | equals 5 | verify-true
get-window -class WizardDialog | get-window -class LinkAnswerDialog | get-group "" -after [get-label Q0] -index 2 
    | get-combo | get-property "getItemCount()" | equals 5 | verify-true
with [get-window -class WizardDialog] {
    with [get-window -class LinkAnswerDialog] {
        get-group "" -after [get-label Q0] -index 1 | get-combo | select Q2
        get-group "" -after [get-label Q0] -index 2 | get-combo | select Q1
        get-button OK | click
    }
    get-button "Link Answer" -index 1 | click
}
get-window -class WizardDialog | get-window -class LinkAnswerDialog | get-group "" -after [get-label Q2] -index 1 
    | get-combo | get-property "getItemCount()" | equals 4 | verify-true
get-window -class WizardDialog | get-window -class LinkAnswerDialog | get-group "" -after [get-label Q2] -index 2 
    | get-combo | get-property "getItemCount()" | equals 4 | verify-true
with [get-window -class WizardDialog] {
    with [get-window -class LinkAnswerDialog] {
        get-group "" -after [get-label Q2] -index 1 | get-combo | select Q1
        get-group "" -after [get-label Q2] -index 2 | get-combo | select "End Questionnaire"
        get-button OK | click
    }
    get-button "Link Answer" -index 2 | click
}
get-window -class WizardDialog | get-window -class LinkAnswerDialog | get-group "" -after [get-label Q1] -index 1 
    | get-combo | get-property "getItemCount()" | equals 3 | verify-true
get-window -class WizardDialog | get-window -class LinkAnswerDialog | get-group "" -after [get-label Q1] -index 2 
    | get-combo | get-property "getItemCount()" | equals 3 | verify-true
