abstract Enum
abstract CAR
name -> string
performance -> integer
abstract BMW_F : CAR
    class -> integer
    power1->Power
abstract TESLA : CAR
    power -> Power
abstract Task
    description -> string
abstract Power : Enum -> integer
    best    : Power = 1
    better  : Power = 2
    good    : Power = 3
    poor    : Power = 4
BMWCars
    F30 : BMW_F
        [ name = "BMW F30" ]
        [ class = 4 ]
        [ power1 = 3 ]
    F87 : BMW_F
        [ name = "BMW F87" ]
        [ performance = 1 ]
        [ class = 2 ]
        [ power1 = 1 ]
TeslaCars
    ModelS : TESLA
        [ name = "Model S" ]
        [ performance = 2 ]
        [ power = 3 ]
Model3 : TESLA
        [ name = "Model 3" ]
        [ performance = 3 ]
        [ power = 1 ]
CarFinder : Task
    [ description = "Find car" ]
    optionPetrol -> BMWCars
        [ power1 < 3 ]
    optionElectric -> TeslaCars